


<%= form_with(model: project_list) do |form| %>


  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>

  <span id="helpBlock" class="help-block"></span>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>



