
    <% if project_list.user == current_user %>
    <tr>
      <td><%= project_list.title %></td>
      <td><%= project_list.description %></td>
      <td><%= project_list.user.email %></td>
      <td><%= link_to 'Show Task', project_list,class: 'css_class', style: 'color:blue' %></td>
      <td><%= link_to 'Edit', edit_project_list_path(project_list),class: 'css_class', style: 'color:yellow' %></td>
      <td><%= link_to 'Delete', project_list, method: :delete, data: { confirm: 'Are you sure?', remote: true },class: 'delete', style: 'color:red' %></td>
    </tr>
    <% end %>

