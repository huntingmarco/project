

<% if current_user %>
<h1>Project Lists</h1>
<table id="project_lists" class="table table-condensed">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    
  <%= render @project_lists %>

  </tbody>
</table>

<br>
<div id="new-project">
<%= link_to("Add New Project", new_project_list_path, remote: true, 
          class: "btn btn-xs btn-success") %>
</div>

<% end %>  